<div class="container">
  <br>
  <br>
  <div id="pins" class="transitions-enabled">
    <% @pins.each do |pin| %>
      <div class="box panel panel-default">
        <%= link_to(image_tag(pin.image.url), pin_path(pin))%>
        <div class="panel-body">
          <h2><%= link_to pin.title, pin_path(pin) %></h2>
        </div>
      </div>
    <% end %>
  </div>
</div>

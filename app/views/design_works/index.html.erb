<div class="container">
  <br>
  <br>
  <div>
    <% @design_works.each do |design_work| %>
      <div class="box panel panel-default">
        <%= link_to(image_tag(design_work.image.url), design_work_path(design_work))%>
        <div class="panel-body">
          <h2><%= link_to design_work.title, design_work_path(design_work) %></h2>
        </div>
      </div>
    <% end %>
  </div>
</div>
